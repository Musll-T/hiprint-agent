{
  "name": "hiprint-agent",
  "version": "1.0.0",
  "description": "Headless print service for vue-plugin-hiprint",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev:frontend": "vite --config src/frontend/vite.config.js",
    "build:frontend": "vite build --config src/frontend/vite.config.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "address": "^2.0.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^11.0.0",
    "conf": "^13.0.0",
    "dayjs": "^1.11.0",
    "express": "^4.18.0",
    "express-session": "^1.19.0",
    "lucide-vue-next": "^0.470.0",
    "naive-ui": "^2.40.0",
    "node-machine-id": "^1.1.12",
    "p-queue": "^8.0.0",
    "prom-client": "^15.0.0",
    "pinia": "^3.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "playwright": "^1.49.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "uuid": "^11.0.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0",
    "zod": "^3.0.0",
    "@asteasolutions/zod-to-openapi": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "optionalDependencies": {
    "bullmq": "^5.0.0",
    "ioredis": "^5.0.0"
  }
}
